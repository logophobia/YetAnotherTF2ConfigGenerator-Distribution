// LAN
alias lan_net "cl_cmdrate 66; cl_interp 0; cl_interp_ratio 0; cl_lagcompensation 0; cl_pred_optimize 2; cl_smooth 0; cl_smoothtime 0.01; cl_updaterate 66; rate 60000"

// Good connection
alias good_net "cl_cmdrate 66; cl_interp 0; cl_interp_ratio 1; cl_lagcompensation 1; cl_pred_optimize 2; cl_smooth 0; cl_smoothtime 0.01; cl_updaterate 66; rate 60000"

// Bad connection
alias bad_net "cl_cmdrate 40; cl_interp 0; cl_interp_ratio 2; cl_lagcompensation 1; cl_pred_optimize 2; cl_smooth 0; cl_smoothtime 0.01; cl_updaterate 40; rate 35000"
